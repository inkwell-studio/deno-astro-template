---
import Box from '@components/infinite-scrolling/box.astro';

export function getStaticPaths(): Array<{ params: { n: number } }> {
    // Accept paths of numbers in the range [1, 10]
    return [...Array(10).keys()].map((i) => ({
        params: { n: ++i },
    }));
}

const { n } = Astro.params;
---
<Box n={Number(n)} />
